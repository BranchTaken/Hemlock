(rule
 (deps
  %{bin:hocc})
 (action
  (with-accepted-exit-codes
   (or 0 1)
   (with-outputs-to help_a.out (run %{bin:hocc} -v -verbose -txt -text -html -hmh -hocc -c -canonical -hm -hemlock -ml -ocaml -s Foo -src Foo -d bar -dstdir bar -h)))))
(rule
 (alias runtest)
 (action (diff help_a.expected help_a.out)))


 (deps
  (glob_files A.hmh*)
  %{bin:hocc})
 (targets A.out.txt A.out.hmh)
 (action
  (with-accepted-exit-codes
   (or 0 1)
   (with-outputs-to A.out (run ./hocc_test %{bin:hocc} A -v -txt -hmh)))))
(rule
 (alias runtest)
 (action
  (progn
   (diff A.expected A.out)
   (diff A.expected.txt A.out.txt)
   (diff A.expected.hmh A.out.hmh))))

